{
  "name": "maisie-frontend",
  "version": 2,
  "public": true,
  "alias": ["heymaisie.com", "www.heymaisie.com"],
  "builds": [
    {"src": "next.config.js", "use": "@now/next"},
    {"src": "static/**", "use": "@now/static"}
  ],
  "routes": [
    {"src": "/static/(.*)", "dest": "/static/$1"},
    { "src": "/verification/(?<token>[^/]*)", "dest": "/verification?token=$token" }
  ],
  "env": {
    "API": "@maisie-backend-url"
  }
}