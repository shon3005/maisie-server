{
  "AWSEBDockerrunVersion": 2,
  "authentication": {
    "bucket": "elasticbeanstalk-us-east-1-640883773139",
    "key": ".dockercfg"
  },
  "containerDefinitions": [
    {
      "name": "client",
      "image": "shon3005/multi-maisie-site",
      "hostname": "client",
      "essential": false,
      "memory": 256
    },
    {
      "name": "api",
      "image": "shon3005/multi-maisie-api",
      "hostname": "api",
      "essential": false,
      "memory": 128
    },
    {
      "name": "nginx",
      "image": "shon3005/multi-maisie-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["client", "api"],
      "memory": 128
    }
  ]
}